{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "./routes/userRoutes",
        "dest": "/api/user"
      },
      {
        "src": "./routes/communityRoutes",
        "dest": "/api/community"
      },
      {
        "src": "/api/community-posts",
        "dest": "/"
      }
    ]
  }
  